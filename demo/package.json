  "devDependencies": {
    "@eclipse-scout/cli": "^23.2.15",
    "@eclipse-scout/tsconfig": "23.2.15",
    "live-server": "^1.2.1"
  },
  "dependencies": {
    "@eclipse-scout/core": "^23.2.15",
    "ace-builds": "1.32.3",
    "jquery": "3.6.0"
  }
}
