{
  "engines": {
    "node": ">=18.12.1",
    "npm": ">=9.1.1",
    "pnpm": ">=7.16.0"
  },
  "main": "src/index.ts",
  "scripts": {
    "pnpm-install": "pnpm install --ignore-scripts",
    "build:dev": "scout-scripts build:dev",
